<%= error_messages_for(@user) %>

<%= form_tag(:controller => 'access', :action => 'attempt_login') do %>
<table>
  <tr>
    <td><%= label_tag(:Email) %></td>
    <td><%= text_field_tag(:email, params[:email], :class => 'input_login' ) %></td>
    <td><%= label_tag(:Password) %></td>
    <td><%= password_field_tag(:password_digest, params[:password_digest], :class => 'input_login') %></td>
  </tr>
  <tr>
    <td colspan="4">
      <center><%= submit_tag("Log in") %></center>
    </td>
  </tr>
</table>
<% end %>
